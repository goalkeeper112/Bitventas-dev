{{#mdl-card title={{name}} }}
  <div class="mdl-card__supporting-text">
    <img src={{photo_1}} alt="" class="img-card" />
    <span>{{description}}</span>
      <p>{{country}}</p>
  </div>
  {{#mdl-card-actions}}
    {{mdl-button text='Ver'}}
  {{/mdl-card-actions}}
  {{#mdl-card-buttons}}
    {{mdl-button icon='share'}}
  {{/mdl-card-buttons}}
{{/mdl-card}}
